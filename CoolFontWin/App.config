<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
            <section name="CFW.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
            <section name="CoolFont.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
            <section name="CoolFontWin.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
            <section name="CoolFontWin.Assets.UserDefaults" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
        </sectionGroup>
      
        <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
      
    </configSections>
  <log4net>

    <root>
      <appender-ref ref="RollingFileAppender" />
      <appender-ref ref="LatestFileAppender" />
      <appender-ref ref="TraceAppender" />
    </root>
    
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="%envFolderPath{MyDocuments}\CoolFontWin\Log.txt" />
      <appendToFile value="true" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="0" />
      <maximumFileSize value="500KB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %level %logger - %message%newline" />
      </layout>
      <threshold value="All" />
    </appender>

    <appender name="LatestFileAppender" type="log4net.Appender.FileAppender">
      <file type="log4net.Util.PatternString" value="%envFolderPath{MyDocuments}\CoolFontWin\LastLog.txt" />
      <appendToFile value="false" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %level %logger - %message%newline" />
      </layout>
      <threshold value="All" />
    </appender>

    <appender name="TraceAppender" type="log4net.Appender.TraceAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%level - %message%newline"/>
      </layout>
      <threshold value="All" />
    </appender>

  </log4net>
    
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="DeltaCompressionDotNet.MsDelta" publicKeyToken="46b2138a390abf55" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.1.0.0" newVersion="1.1.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  <loadFromRemoteSources enabled="true"/>
  </runtime>
  
  <userSettings>
    <CFW.Properties.Settings>
      <setting name="VJoyDir" serializeAs="String">
        <value>C:\\Program Files\\vJoy\\x64</value>
      </setting>
      <setting name="FirstInstall" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="RunForwardKey" serializeAs="String">
        <value>87</value>
      </setting>
      <setting name="ConnectedDevices" serializeAs="Xml">
        <value>
          <ArrayOfString xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <string>Primary</string>
          </ArrayOfString>
        </value>
      </setting>
      <setting name="VJoyID" serializeAs="String">
        <value>0</value>
      </setting>
      <setting name="LastPort" serializeAs="String">
        <value>0</value>
      </setting>
    </CFW.Properties.Settings>
    <CoolFont.Properties.Settings>
      <setting name="VJoyDir" serializeAs="String">
        <value>C:\\Program Files\\vJoy</value>
      </setting>
      <setting name="FirstInstall" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="RunForwardKey" serializeAs="String">
        <value>87</value>
      </setting>
      <setting name="ConnectedDevices" serializeAs="Xml">
        <value>
          <ArrayOfString xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <string>Primary</string>
          </ArrayOfString>
        </value>
      </setting>
      <setting name="JustUpdated" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="VJoyID" serializeAs="String">
        <value>1</value>
      </setting>
    </CoolFont.Properties.Settings>
    <CoolFontWin.Properties.Settings>
      <setting name="VJoyDir" serializeAs="String">
        <value>%PROGRAMFILES%\\vjoy\\</value>
      </setting>
      
    </CoolFontWin.Properties.Settings>
    <CoolFontWin.Assets.UserDefaults>
      <setting name="VJoyDirectory" serializeAs="String">
        <value>%PROGRAMFILES%\\vjoy\\</value>
      </setting>
      
    </CoolFontWin.Assets.UserDefaults>
  </userSettings>
  
</configuration>

